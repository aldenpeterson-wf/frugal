      "command": [
        "/usr/bin/java",
        "-Djava.security.egd=file:///dev/urandom",
        "-cp",
        "cross.jar",
        "com.workiva.TestClient"
      ],
      "transports": [
        "stateless",
        "stateful",
        "stateless-stateful",
        "http"
      ]
