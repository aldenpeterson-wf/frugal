[
  {
    "name": "dart",
    "client": {
      "timeout": 15,
      "transports": [
        "buffered",
        "http"
      ],
      "sockets": [
        "ip"
      ],
      "protocols": [
        "binary",
        "compact",
        "json"
      ],
      "command": [
        "dart",
        "test_client/bin/main.dart"
      ]
    },
    "workdir": "dart"
  },
  {
    "name": "go",
    "server": {
      "delay": 5,
      "command": [
        "testserver"
      ]
    },
    "client": {
      "timeout": 6,
      "command": [
        "testclient"
      ]
    },
    "transports": [
      "buffered"
    ],
    "sockets": [
      "ip"
    ],
    "protocols": [
      "binary",
      "compact",
      "json"
    ],
    "workdir": "go/bin"
  },
  {
    "name": "java",
    "join_args": true,

    "command": [
      "mvn",
      "exec:java"
    ],
    "client": {
      "timeout": 20,
      "command": [
        "\"-Dexec.mainClass=com.workiva.TestClient\""
      ],
      "extra_args": [
        "-f=\"java/frugal-integration-test/pom.xml\""
      ]
    },
    "server": {
      "delay": 15,
      "command": [
        "\"-Dexec.mainClass=com.workiva.TestServer\""
      ],
      "extra_args": [
        "-f=\"java/frugal-integration-test/pom.xml\""
      ]
    },
    "transports": [
      "buffered",
      "framed"
    ],
    "sockets": [
      "ip"
    ],
    "protocols": [
      "binary",
      "compact"
    ]
  }
]
