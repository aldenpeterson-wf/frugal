[
  {
    "name": "py:tornado",
    "client": {
      "timeout": 7,
      "command": [
        "python",
        "-u",
        "client.py"
      ]
    },
    "server": {
      "delay": 7,
      "command": [
        "python",
        "server.py"
      ]
    },
    "transports": [
      "stateless",
      "stateful",
      "stateless-stateful",
      "http"
    ],
    "sockets": [
      "ip"
    ],
    "protocols": [
      "binary",
      "compact",
      "json"
    ],
    "workdir": "python/tornado"
  },
  {
    "name": "py:asyncio",
    "client": {
      "timeout": 7,
      "command": [
        "python3.5",
        "-u",
        "client.py"
      ]
    },
    "transports": [
      "stateless"
    ],
    "sockets": [
      "ip"
    ],
    "protocols": [
      "binary"
    ],
    "workdir": "python/asyncio"

  }
]
